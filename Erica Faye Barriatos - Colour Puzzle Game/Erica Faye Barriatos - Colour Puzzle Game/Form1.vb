    Private Sub Timer1_Tick(sender As System.Object, e As System.EventArgs) Handles Timer1.Tick
        seconds = seconds + 1
        If seconds = 60 Then
            minutes = minutes + 1
            seconds = seconds - 60
        End If
        lblTime.Text = minutes & " : " & seconds
    End Sub

